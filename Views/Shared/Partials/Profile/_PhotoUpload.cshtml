@model List<Photo>
<div class="row mt-3">
    <div class="col-5">
        <form asp-action="AddPhoto" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label for="file">Upload Photo</label>
                <input type="file" name="file" id="file" class="form-control" accept="image/*" required />
            </div>
            <button type="submit" class="btn btn-primary mt-3">Upload</button>
        </form>

    </div>
    <div class="col-7">
        <div class="row">
            @foreach (Photo p in Model)
            {
            <div class="col-4 mt-3">
                <img class="rounded" src="@p.PhotoUrl" style="width: 100%;" alt="photo">
            </div>
            }
        </div>
    </div>
</div>
