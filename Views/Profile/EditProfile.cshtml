@model User
@{
    EditUser editedUser = new EditUser()
    {
        FirstName = Model.FirstName,
        LastName = Model.LastName,
        UserName = Model.UserName,
        Occupation = Model.Occupation,
        Gender = Model.Gender,
        RelationshipStatus = Model.RelationshipStatus,
        DateOfBirth = Model.DateOfBirth,
        Address = Model.Address,
        City = Model.City,
        State = Model.State,
        Country = Model.Country,
    };
}
<div class="col-8 mt-3">
        <ul class="nav nav-tabs" role="tablist">
            <li class="nav-item" role="presentation">
                <a class="nav-link active" data-bs-toggle="tab" href="#home" aria-selected="true" role="tab">Upload Photos</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link" data-bs-toggle="tab" href="#EditProfile" aria-selected="false" role="tab"
                    tabindex="-1">Edit Bio</a>
            </li>

        </ul>
        <div id="myTabContent" class="tab-content">
            <div class="tab-pane fade active show" id="home" role="tabpanel">
                @await Html.PartialAsync("../Shared/Partials/Profile/_PhotoUpload", Model.Photos)
            </div>
            <div class="tab-pane fade" id="EditProfile" role="tabpanel">
                @await Html.PartialAsync("../Shared/Partials/Profile/_EditProfile", editedUser)
            </div>
        </div>
    </div>
